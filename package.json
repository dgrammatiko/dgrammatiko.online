{
  "name": "dgrammatiko.online",
  "private": true,
  "version": "0.0.1",
  "description": "Personal site",
  "main": "build.js",
  "scripts": {
    "copy-assets": "node ./build-scripts/copy-assets.js",
    "deploy-patch": "npm version patch --force -m \"version %s\"",
    "postversion": "git push && git push --tags",
    "deploy": "node ./build-scripts/deploy.js",
    "build1": "node ./build-scripts/clean.js && node ./build-scripts/copy-assets.js && node ./build-scripts/copy-static.js && node ./build-scripts/glob/buildAllCss.js && node ./build-scripts/buildjs.js && node ./build-scripts/responsive-images.js",
    "production": "npm run build && npx eleventy",
    "dev": "npm run build && npx eleventy --serve",
    "watch": "ELEVENTY_ENV=watch npx rollup -c rollup.config.js && node ./build-scripts/copy-assets.js && node ./build-scripts/copy-static.js && node ./build-scripts/responsive-images.js && node ./build-scripts/postcss-process.js && npx eleventy --watch --serve",
    "build": "npx rollup -c rollup.config.js && node ./build-scripts/copy-assets.js && node ./build-scripts/copy-static.js && node ./build-scripts/responsive-images.js && node ./build-scripts/postcss-process.js && npx eleventy"
  },
  "author": "dgrammatiko",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "0.10.0",
    "@11ty/eleventy-plugin-rss": "1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.0.1",
    "@babel/core": "7.8.7",
    "@babel/preset-env": "7.8.7",
    "@gerhobbelt/markdown-it-responsive": "latest",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "@rollup/plugin-replace": "2.3.1",
    "@tbranyen/jsdom": "13.0.0",
    "cssnano": "4.1.10",
    "fs-extra": "9.0.0",
    "gh-pages": "2.2.0",
    "glob": "7.1.6",
    "html-minifier": "4.0.0",
    "http-server": "0.12.1",
    "image-size": "0.8.3",
    "lodash": "4.17.15",
    "luxon": "latest",
    "markdown-it-anchor": "5.2.5",
    "mkdirp": "1.0.3",
    "path": "0.12.7",
    "postcss": "7.0.27",
    "postcss-cli": "7.1.0",
    "postcss-custom-media": "7.0.8",
    "postcss-custom-selectors": "5.1.2",
    "postcss-discard-comments": "4.0.2",
    "postcss-easy-import": "3.0.0",
    "postcss-import": "12.0.1",
    "postcss-mixins": "6.2.3",
    "postcss-nested": "4.2.1",
    "postcss-preset-env": "6.7.0",
    "rename": "1.0.4",
    "require-dir": "latest",
    "rmrf": "2.0.2",
    "rollup": "2.1.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-terser": "5.3.0",
    "sharp": "0.25.1",
    "terser": "4.6.7",
    "workbox-cacheable-response": "5.0.0"
  },
  "dependencies": {
    "@stardazed/streams": "3.0.0",
    "@stardazed/streams-fetch-adapter": "3.0.0",
    "@stardazed/streams-polyfill": "2.1.0",
    "ce-theme-switcher": "0.0.3",
    "intersection-observer": "0.7.0",
    "watch": "1.0.2",
    "web-streams-polyfill": "2.1.0",
    "workbox-core": "5.0.0",
    "workbox-precaching": "5.0.0",
    "workbox-routing": "5.0.0",
    "workbox-strategies": "5.0.0",
    "workbox-streams": "5.0.0"
  }
}
